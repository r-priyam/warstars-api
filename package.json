{
    "name": "warstars-api",
    "version": "0.1.16",
    "private": true,
    "description": "API for WarStars",
    "license": "MIT",
    "author": "Priyam",
    "scripts": {
        "prebuild": "rimraf dist",
        "build": "nest build",
        "format": "prettier --write .",
        "lint": "eslint \"**/*.{ts,js}\"",
        "lint:fix": "eslint \"**/*.{ts,js}\" --fix",
