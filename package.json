        "@types/node-fetch": "2.6.12",
        "@types/pg": "^8.15.4",
        "@types/uid-safe": "^2.1.5",
        "bumpp": "^8.2.1",
        "eslint": "^8.57.1",
        "eslint-config-prettier": "^8.10.2",
