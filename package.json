        "@nestjs/platform-fastify": "^8.4.3",
        "@nestjs/typeorm": "^8.0.3",
        "@ogma/nestjs-module": "^3.2.0",
        "@ogma/platform-fastify": "^3.0.1",
        "clashofclans.js": "^2.6.1",
        "class-transformer": "^0.5.1",
        "class-validator": "^0.13.2",
        "connect-typeorm": "^1.1.4",
        "crypto-js": "^4.1.1",
        "discord-akairo": "github:csuvajit/discord-akairo#main",
